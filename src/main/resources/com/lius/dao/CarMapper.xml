<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lius.dao.CarMapper">
    <resultMap id="Temp" type="com.lius.entity.Temp">
        <id property="id" column="id"/>
        <result property="carName" column="car_name"/>
        <result property="carDesc" column="car_desc"/>
        <result property="carStatus" column="car_status"/>
        <collection property="users" ofType="com.lius.entity.User">
            <id property="id" column="uid"/>
            <result property="userName" column="user_name"/>
        </collection>
    </resultMap>
</mapper>